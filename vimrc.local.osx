source $VIMRUNTIME/mswin.vim
behave mswin
set langmenu=en_US
let $LANG = 'en_US'
source $VIMRUNTIME/delmenu.vim
source $VIMRUNTIME/menu.vim
colorscheme molokai
set wrap
set fileencodings=ucs-bom,utf-8,cp98030,big5,euc-jp,euc-kr,latin1
set nofoldenable
let g:DisableAutoPHPFolding = 1
set guifont=Monaco:h15
set lines=43
set columns=120
let g:tex_conceal = ""
